  <!--  搜索已开立信息-->
  <select id="searchRegisterByasenumber" resultMap="RegisterPayMap">
    SELECT r.*,f.id as fid,f.itemname,f.price,c.state,c.creationtime,c.num,c.id as cid
    FROM Register r
    join checkapply c
    on r.id = c.RegistID
    join fmeditem f
    on f.id = c.ItemID
	WHERE Casenumber=#{casenumber} and state = 2
  </select>


<!--  搜索已交费信息-->
  <select id="searchRegisterRefundByasenumber" resultMap="RegisterPayMap">
    SELECT r.*,f.id as fid,f.itemname,f.price,c.state,c.creationtime,c.num,c.id as cid
